#!/bin/bash
#SBATCH --job-name=muppet
#SBATCH -p gpu
#SBATCH -n 10
#SBATCH -G a100:4
#SBATCH --nodes=1
#SBATCH --mem=40GB
#SBATCH --time=0-05:00:00
#SBATCH --account=etechnik_gpu
#SBATCH -o mtl_logs/%x-%j.out

MTL_PATH=/beegfs/zhukova/magpie-multi-task
CWD=$(pwd)
module load 2021a Anaconda3/2021.05
module load 2021a GCC/10.3.0
module load 2021a Python/3.9.5
module load 2021a CUDA/11.4.2

source activate mtl_training_env
cd /beegfs/zhukova/magpie-multi-task
export PYTHONPATH=$PYTHONPATH:$MTL_PATH
export TOKENIZERS_PARALLELISM=true
export WANDB_DISABLE_SERVICE=true
python scripts/evaluation_robust/run_30_softhard_magpie.py